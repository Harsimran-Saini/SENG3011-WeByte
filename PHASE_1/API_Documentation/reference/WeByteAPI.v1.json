          "influenza a/h3n5",
          "influenza a/h3n2",
          "influenza a/h2n2",
          "hand",
          "foot and mouth disease",
          "hantavirus",
          "hepatitis a",
          "hepatitis b",
          "hepatitis c",
          "hepatitis d",
          "hepatitis e",
          "histoplasmosis",
          "hiv/aids",
          "lassa fever",
          "malaria",
          "marburg virus disease",
          "measles",
          "mers-cov",
          "mumps",
          "nipah virus",
          "norovirus infection",
          "pertussis",
          "plague",
          "pneumococcus pneumonia",
          "poliomyelitis",
          "q fever",
