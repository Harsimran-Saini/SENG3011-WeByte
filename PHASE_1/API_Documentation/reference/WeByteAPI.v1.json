  "paths": {
    "/search_keywords": {
      "parameters": [],
      "get": {
        "summary": "Get Search results from keyword",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/article"
                  }
                },
                "examples": {
                  "No results": {
                    "value": []
                  },
                  "Covid": {
                    "value": [
                      {
                        "url": "https://promedmail.org/promed-post/?id=8224463",
                        "date_of_publication": "2021-03-03 14:04:20",
                        "headline": "CORONAVIRUS DISEASE 2019 UPDATE (85): USA CHALLENGES IN DISEASE CONTROL, BRAZIL REINFECTION, WHO, GLOBAL",
                        "main_text": "Over the weekend both the Food and Drug Administration (FDA) and the Centers for Disease Control and Prevention (CDC) gave the green light to Johnson & Johnson's COVID-19 vaccine etc...",
                        "reports": [
                          {
                            "diseases": [
                              "COVID-19"
                            ],
                            "event_date": [
                              "2021-03-01 xx-xx-xx"
                            ],
                            "locations": [
                              {
                                "country": "USA",
                                "location": "USA"
                              }
                            ]
                          },
                          {
                            "diseases": [
                              "COVID-19"
                            ],
                            "event_date": [
                              "2021-01-21 xx-xx-xx"
                            ],
                            "locations": [
                              {
                                "country": "Brazil",
                                "location": "Salvador"
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            }
          },
          "400": {
            "description": "User error - please ensure you have the required headers"
          }
        },
        "operationId": "get-search-results",
        "description": "Note: This endpoint is not final, and serves demonstration purposes only\n\nGetting a list of articles matching the keyword(s)\n\nExample usage:\nGET: www.webyte.com/search_keywords?keywords=COVID-19&date-range=2021-03-03&location=USA",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "keywords",
            "description": "Comma seperated list of keywords",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "date-range",
            "description": "Date range"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "location",
            "description": "Location in"
          }
        ]
      }
    }
  },
