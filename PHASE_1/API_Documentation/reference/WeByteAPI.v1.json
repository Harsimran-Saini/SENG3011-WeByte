{
  "openapi": "3.0.0",
  "tags": [
    {
      "name": "epidemic"
    },
    {
      "name": "student"
    }
  ],
  "info": {
    "title": "WeByte API - ProMedMail",
    "description": "RESTful API for the retrieval of data from promedmail.org",
    "version": "0.1",
    "contact": {
      "name": "We Byte!",
      "email": "z5208912@ad.unsw.edu.au"
    }
  },
  "servers": [
    {
      "url": "WeByte.com",
      "description": "Fictional Server Addr"
    }
  ],
  "paths": {
    "/search_keywords": {
      "parameters": [],
      "get": {
        "summary": "Get Search results from keyword",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "headers": {},
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/article"
                  }
                },
                "examples": {
                  "No results": {
                    "value": []
                  },
                  "covid": {
                    "value": [
                      {
                        "url": "https://promedmail.org/promed-post/?id=8224463",
                        "date_of_publication": "2021-03-03 14:04:20",
                        "headline": "CORONAVIRUS DISEASE 2019 UPDATE (85): USA CHALLENGES IN DISEASE CONTROL, BRAZIL REINFECTION, WHO, GLOBAL",
                        "main_text": "Over the weekend both the Food and Drug Administration (FDA) and the Centers for Disease Control and Prevention (CDC) gave the green light to Johnson & Johnson's COVID-19 vaccine etc...",
                        "reports": [
                          {
                            "diseases": [
                              "COVID-19"
                            ],
                            "syndromes": [
                              "Fever of unknown Origin",
                              "Meningitis"
                            ],
                            "event_date": [
                              "2021-xx-xx xx:xx:xx"
                            ],
                            "locations": [
                              {
                                "country": "Brazil",
                                "location": ""
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            }
          },
          "400": {
            "description": "User error - please ensure you have the required headers"
          }
        },
        "operationId": "get-search-results",
        "description": "Getting a list of articles matching the keyword(s)",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "key_terms",
            "description": "Comma seperated list of keywords"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "location",
            "description": "Location to search for"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "start_date",
            "description": "Start date for filtering search results",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "end_date",
            "description": "End date for filtering search results",
            "required": true
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "timezone",
            "description": "Timezone to search with"
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "article": {
        "title": "article",
        "type": "object",
        "description": "Article on promedmail.org",
        "x-examples": {
          "covid": {
            "url": "https://promedmail.org/promed-post/?id=8224463",
            "date_of_publication": "2021-03-03 14:04:20",
            "headline": "CORONAVIRUS DISEASE 2019 UPDATE (85): USA CHALLENGES IN DISEASE CONTROL, BRAZIL REINFECTION, WHO, GLOBAL",
            "main_text": "Over the weekend both the Food and Drug Administration (FDA) and the Centers for Disease Control and Prevention (CDC) gave the green light to Johnson & Johnson's COVID-19 vaccine etc...",
            "reports": [
              {
                "diseases": [
                  "COVID-19"
                ],
                "syndromes": [
                  "Fever of unknown Origin",
                  "Meningitis"
                ],
                "event_date": [
                  "2021-xx-xx xx:xx:xx"
                ],
                "locations": [
                  {
                    "country": "Brazil",
                    "location": ""
                  }
                ]
              }
            ]
          }
        },
        "properties": {
          "url": {
            "type": "string"
          },
          "date_of_publication": {
            "$ref": "#/components/schemas/date"
          },
          "headline": {
            "type": "string"
          },
          "main_text": {
            "type": "string"
          },
          "reports": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/report"
            }
          }
        }
      },
      "report": {
        "title": "report",
        "type": "object",
        "x-examples": {
          "example-1": {
            "diseases": [
              "unknown"
            ],
            "syndromes": [
              "Haemorrhagic Fever"
            ],
            "event_date": [
              "string"
            ],
            "locations": [
              {
                "country": "string",
                "location": "string"
              }
            ]
          }
        },
        "description": "An object containing information about a case mentioned in the article",
        "properties": {
          "diseases": {
            "type": "array",
            "description": "The array of diseases",
            "items": {
              "$ref": "#/components/schemas/disease"
            }
          },
          "syndromes": {
            "type": "array",
            "description": "The array of syndromes",
            "items": {
              "$ref": "#/components/schemas/syndromes"
            }
          },
          "event_date": {
            "type": "array",
            "description": "The date on which the case occured (not the publication date of the\narticle).",
            "items": {
              "type": "string"
            }
          },
          "locations": {
            "type": "array",
            "description": "An array of all the locations mentioned in the article referring to this\nparticular case.",
            "items": {
              "$ref": "#/components/schemas/location"
            }
          }
        }
      },
      "location": {
        "title": "location",
        "type": "object",
        "x-examples": {
          "sydney": {
            "country": "australia",
            "location": "sydney"
          }
        },
        "description": "Location of the case",
        "properties": {
          "country": {
            "type": "string",
            "description": "country name"
          },
          "location": {
            "type": "string",
            "description": "any other details on the location"
          }
        }
      },
      "date-exact": {
        "type": "string",
        "title": "date-exact",
        "pattern": "^(\\d{4})-(\\d\\d|xx)-(\\d\\d|xx) (\\d\\d|xx):(\\d\\d|xx):(\\d\\d|xx)$",
        "x-examples": {
          "2018": "2018-xx-xx xx:xx:xx",
          "1 November 2018": "2018-11-01 xx:xx:xx",
          "5 o'clock November 2018": "2018-11-xx 17:00:xx"
        },
        "description": "Exact date you want to search"
      },
      "date-range": {
        "type": "string",
        "title": "date-range",
        "description": "Range of dates to search between (Ensure the earlier date comes first!)",
        "pattern": "^(\\d{4})-(\\d\\d|xx)-(\\d\\d|xx) (\\d\\d|xx):(\\d\\d|xx):(\\d\\d|xx) to (\\d{4})-(\\d\\d|xx)-(\\d\\d|xx) (\\d\\d|xx):(\\d\\d|xx):(\\d\\d|xx)$",
        "x-examples": {
          "2018 to 2019": "2018-xx-xx xx:xx:xx to 2019-xx-xx xx:xx:xx",
          "5'o clock 1 November 2018 to December 2018": "2018-11-01 17:xx:xx to 2018-12-xx xx:xx:xx"
        }
      },
      "date": {
        "oneOf": [
          {
            "$ref": "#/components/schemas/date-exact"
          },
          {
            "$ref": "#/components/schemas/date-range"
          }
        ],
        "x-examples": {
          "Date-exact": "2018-xx-xx xx:xx:xx",
          "Date-range": "2018-xx-xx xx:xx:xx to 2019-xx-xx xx:xx:xx"
        },
        "title": "",
        "description": "Either date-range or date-exact"
      },
      "disease": {
        "type": "string",
        "x-examples": {
          "Unknown": "unknown"
        },
        "enum": [
          "unknown",
          "other",
          "anthrax cutaneous",
          "anthrax gastrointestinous",
          "anthrax inhalation",
          "botulism",
          "brucellosis",
          "chikungunya",
          "cholera",
          "cryptococcosis",
